{
  "name": "wtix-backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "dev": "npm run prisma:init && nodemon --exec npx ts-node ./src/server.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "prisma:init": "prisma generate && prisma migrate deploy && prisma db seed",
    "liveDev": "npm run devUp; npm run devDown",
    "devUp": "docker compose --profile server_dev up -d && sleep 60 && cross-env NODE_ENV=DEV npm run dev",
    "devDown": "docker compose --profile server_dev down",
    "composeBuild": "docker compose --profile full_compose build --no-cache",
    "composeUp": "docker compose --profile full_compose up -d",
    "composeDown": "docker compose --profile full_compose down -v"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "@prisma/client": "^4.14.1",
    "@types/supertest": "^2.0.12",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-oauth2-jwt-bearer": "^1.1.0",
    "express-session": "^1.17.3",
    "helmet": "^5.1.0",
    "js-yaml": "^4.1.0",
    "json-refs": "^3.0.15",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "stripe": "^9.10.0",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "tslib": "^2.4.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^28.1.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.16.16",
    "@types/pg": "^8.6.5",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/ws": "^8.5.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "axios": "^0.27.2",
    "eslint": "^8.18.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.18",
    "prisma": "^4.14.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
